{
  "name": "web-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack/webpack.prod.js",
    "analyze": "cross-env NODE_ENV=production webpack --profile --json --config config/webpack/webpack.prod.js > stats.json",
    "ts-check-hook": "tsc --noEmit -p tsconfig.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "prepare": "husky install",
    "cz": "npx cz-customizable"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn run lint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/git/commitizen.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@loadable/component": "^5.15.2",
    "@mui/icons-material": "^5.5.1",
    "@mui/lab": "^5.0.0-alpha.77",
    "@mui/material": "^5.5.3",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.11.1",
    "mediasoup-client": "^3.6.51",
    "mobx": "^6.5.0",
    "mobx-react-lite": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.29.0",
    "react-router-dom": "^6.3.0",
    "reflect-metadata": "^0.1.13",
    "socket.io-client": "^4.4.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@commitlint/cli": "^17.0.3",
    "@loadable/babel-plugin": "^5.13.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/jest": "^28.1.8",
    "@types/loadable__component": "^5.13.4",
    "@types/node": "^18.7.13",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-loader": "^8.2.5",
    "circular-dependency-plugin": "^5.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "cz-customizable": "^6.3.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.6.1",
    "react-refresh": "^0.14.0",
    "terser-webpack-plugin": "^5.3.5",
    "ts-loader": "^9.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0",
    "webpack-merge": "^5.8.0"
  }
}
